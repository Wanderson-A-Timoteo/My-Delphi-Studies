  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Principal, Vcl.StdCtrls;

type
  TForm1 = class(TForm)
    Eventos1: TEventos;
    Memo1: TMemo;
    procedure Eventos1Status(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

